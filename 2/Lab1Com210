import java.util.Scanner;
public class lab1Com210pt3{

       public static void main(String[]args{
       Scanner kb = new Scanner (System.in);
       
       int num;
       
       System.out.printIn("Please enter a number");
       num=kb.nextInt();
       
       if((num>40)||(num<10))
       {//start if 1
          System.out.printIn("number is out of bounds");
          }//end if 1
          
          else
          
          {//start else
             if(num%2==0)
             System.out.printIn("The number is in bounds and even");
          }//end else
             
   }
   
   //Question5;
   
   public static void main(String[]args){
          for(int i=0; i<=1;i++)
              {
          for(int j=0;j<=1;j++)
          
              {
                System.out.printIn(i+""+j);
              }
          }
      }
  }
  
  
